<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/logo_bar_color">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/Reservation_detail_list"
                    android:textSize="20dp"
                    android:fontFamily="@font/hanal"
                    android:textAlignment="center"
                    android:padding="10dp"
                    android:layout_marginBottom="50dp"
                    android:textColor="@color/Base_text_color"
                    android:background="@drawable/main_textview_logo"
                    tools:ignore="MissingConstraints"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_page_reservation_element01"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/my_page_element_list"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:padding="12dp"
                    android:text="예매번호" />

                <TextView
                    android:id="@+id/Reservation_id"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:gravity="end"
                    android:padding="12dp"
                    android:text="1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_page_reservation_element02"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/my_page_element_list"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:padding="12dp"
                    android:text="예매일" />

                <TextView
                    android:id="@+id/Reservation_date"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:gravity="end"
                    android:padding="12dp"
                    android:text="2022-11-09" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_page_reservation_element03"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/my_page_element_list"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:padding="12dp"
                    android:text="극장명" />

                <TextView
                    android:id="@+id/Reservation_concert_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:gravity="end"
                    android:padding="12dp"
                    android:text="궁동예술극장" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_page_reservation_element04"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/my_page_element_list"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:padding="12dp"
                    android:text="연극명" />

                <TextView
                    android:id="@+id/Reservation_play_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:gravity="end"
                    android:padding="12dp"
                    android:text="연극명" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_page_reservation_element05"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/my_page_element_list"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:padding="12dp"
                    android:text="상영시작시간" />

                <TextView
                    android:id="@+id/Reservation_start_date"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:gravity="end"
                    android:padding="12dp"
                    android:text="연극시작" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_page_reservation_element06"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/my_page_element_list"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:padding="12dp"
                    android:text="상영종료시간" />

                <TextView
                    android:id="@+id/Reservation_end_date"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:gravity="end"
                    android:padding="12dp"
                    android:text="연극종료"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_page_reservation_element07"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/my_page_element_list"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:padding="12dp"
                    android:text="결제금액" />

                <TextView
                    android:id="@+id/Reservation_price"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:gravity="end"
                    android:padding="12dp"
                    android:text="결제금액"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_page_reservation_element08"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/my_page_element_list"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:padding="12dp"
                    android:text="예약 상태" />

                <TextView
                    android:id="@+id/Reservation_state"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:gravity="end"
                    android:padding="12dp"
                    android:text="예약 상태"/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>